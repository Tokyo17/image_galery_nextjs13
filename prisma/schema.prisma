generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model galery {
  id      Int      @id @default(autoincrement())
  name  String
  url   String   @db.VarChar(255) @unique
  caption String?
}
